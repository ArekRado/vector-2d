{
  "name": "@arekrado/vector-2d",
  "version": "0.0.8",
  "description": "Pure functions to manipulate 2d vectors",
  "umd:main": "dist/vector-2d.umd.production.js",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "index.js",
  "npmName": "@arekrado/vector-2d",
  "sideEffects": false,
  "scripts": {
    "test": "jest ./src --config=jest.config.js --watch=false --coverage",
    "test:watch": "jest ./src --config=jest.config.js --watch --coverage=false",
    "build:pure": "./node_modules/typescript/bin/tsc --project tsconfig.build.json",
    "build": "rm -rf dist & npm run build:pure",
    "publish:local": "yalc publish",
    "example": "cd example && parcel index.html"
  },
  "lint-staged": {
    "**/*.{ts,tsx,md,mdx,js,jsx}": [
      "npm run format",
      "tsdx lint",
      "git add"
    ]
  },
  "engines": {
    "node": ">=16",
    "npm": ">=8.3.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ArekRado/vector-2d.git"
  },
  "keywords": [
    "vector",
    "2d",
    "Functional programming",
    "FP",
    "pure"
  ],
  "author": "Arek Rado",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ArekRado/vector-2d/issues"
  },
  "homepage": "https://github.com/ArekRado/vector-2d#readme",
  "devDependencies": {
    "@babel/core": "7.16.5",
    "@babel/preset-env": "7.16.5",
    "@babel/preset-typescript": "7.16.5",
    "@types/jest": "27.0.3",
    "babel-jest": "27.4.5",
    "jest": "27.4.5",
    "prettier": "2.5.1",
    "typescript": "4.5.4"
  }
}
